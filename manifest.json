{
  "manifest_version": 3,
  "name": "Text Extractor",
  "description": "Extracts text from the current open tab made by me for your educational purpose only.",
  "version": "69",
    "author": "mailidleboy@gmai.com",
    "icons": {
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "128": "icon128.png"
    },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "extract-text": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      },
      "description": "Extract Text"
    }
  }
}
